<krpano version="1.18">
	<plugin name="video"
        url.flash="videoplayer.swf"
        url.html5="videoplayer.js"
        videourl="video.m4v|video.webm"
        posterurl="videoposter.jpg"
        panovideo="true"
        pausedonstart="false"
        loop="false"
        volume="1.0"
        onvideoready=""
        onvideoplay=""
        onvideopaused=""
        onvideocomplete=""
        onerror=""
        />
	<!-- the skin -->
	<include url="skin/defaultskin.xml" />
	
	<!-- view settings -->
	<view hlookat="0" vlookat="0" maxpixelzoom="1.0" fovmax="150" limitview="range" vlookatmin="-79.781" vlookatmax="79.781" />
	

	<preview url="__________________.tiles/preview.jpg" />

	<image>
		<cube url="__________________.tiles/pano_%s.jpg" />

		<mobile>
			<cube url="__________________.tiles/mobile_%s.jpg" />
		</mobile>
	</image>

</krpano>
